{
  "id": "Employee",
  "table": "employees",
  "title": "직원",
  "props": [
    { "name": "id", "type": "integer", "unsigned": true, "desc": "ID" },
    {
      "name": "created_at",
      "type": "timestamp",
      "desc": "등록일시",
      "dbDefault": "CURRENT_TIMESTAMP"
    },
    {
      "type": "relation",
      "name": "user",
      "with": "User",
      "desc": "USER",
      "relationType": "OneToOne",
      "hasJoinColumn": true,
      "onUpdate": "CASCADE",
      "onDelete": "CASCADE"
    },
    {
      "type": "relation",
      "name": "department",
      "with": "Department",
      "nullable": true,
      "desc": "부서",
      "relationType": "BelongsToOne",
      "onUpdate": "CASCADE",
      "onDelete": "SET NULL"
    },
    {
      "name": "employee_number",
      "desc": "사번",
      "type": "string",
      "length": 32
    },
    {
      "name": "salary",
      "nullable": true,
      "desc": "SALARY",
      "type": "decimal",
      "precision": 10,
      "scale": 2
    }
  ],
  "indexes": [],
  "subsets": {
    "A": [
      "id",
      "created_at",
      "user.id",
      "user.username",
      "department.id",
      "department.name",
      "department.company.name",
      "employee_number",
      "salary"
    ]
  },
  "enums": {
    "EmployeeOrderBy": { "id-desc": "ID최신순" },
    "EmployeeSearchField": { "id": "ID" }
  }
}
